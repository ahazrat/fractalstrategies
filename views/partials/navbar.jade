- var brand = "<fractalStrategies>"
nav(class='navbar navbar-default')
    .container-fluid
    
        // Brand and toggle get grouped for better mobile display
        .navbar-header
            button(type='button', class='navbar-toggle collapsed', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1', aria-expanded="false")
                span(class='sr-only') Toggle navigation
                span(class='icon-bar')
                span(class='icon-bar')
                span(class='icon-bar')
            a(class="navbar-brand", href="/")= brand
        
        // Collect the nav links, forms, and other content for toggling
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
            ul(class='nav navbar-nav')
                li(class='active'): a(href='contact') Contact
                    span(class='sr-only') (current)
                li: a(href='tech') Technologies
                li(class='dropdown')
                    a(href='#', class='dropdown-toggle', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') Projects
                        span(class='caret')
                    ul(class='dropdown-menu')
                        li: a(href='tasktree') TaskTree
                        li: a(href='reddit') Reddit Metrics
                        li: a(href='democracy') Direct Democracy
                        li: a(href='orders') Order Kiosk
                        li(role='separator', class='divider')
                        li: a(href='compute') Project Compute
                        li(role='separator', class='divider')
                        li: a(href='http://www.asifandtuba.com') Asif and Tuba
            form(class='navbar-form navbar-left', role='search')
                .form-group
                    input(type='text', class='form-control', placeholder='search strategies')
                button(type='submit', class='btn btn-default') Submit
            ul(class='nav navbar-nav navbar-right')
                li: a(href='signup') Signup
                li(class='dropdown')
                    a(href='#', class='dropdown-toggle', data-toggle='dropdown', role='button', aria-haspopup='true', aria-expanded='false') User
                        span(class='caret')
                    ul(class='dropdown-menu')
                        li: a(href='profile') Profile
                        li: a(href='settings') Settings
                        li: a(href='help') Help
                        li(role='separator', class='divider')
                        li: a(href='/') Logout